{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "crons": [
    {
      "path": "/cron/predict",
      "schedule": "0 */3 * * *"
    },
    {
      "path": "/cron/results",
      "schedule": "0/5 * * * *"
    },
    {
      "path": "/cron/withdraw-autobet",
      "schedule": "0 */4 * * *"
    }
  ]
}